<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="imagebutton">
        <item name="android:layout_width">105dp</item>
        <item name="android:layout_height">105dp</item>
        <item name="android:textColor">#000000</item>
        <item name="android:background">@xml/redondeado</item>
        <item name="android:textSize">35dp</item>
        <item name="android:gravity">center</item>
    </style>

    <style name="textView">
        <item name="android:textColor">@color/blue_text</item>
        <item name="android:textSize">@dimen/_11sdp</item>
        <item name="android:padding">20dp</item>
        <item name="android:layout_width">830dp</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

    <style name="text_email_title">
        <item name="android:paddingBottom">@dimen/down</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">@dimen/_13sdp</item>
        <item name="android:textColor">@color/blue_text</item>
        <item name="android:gravity">center</item>
    </style>
    
    <style name="linerLayout">
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:orientation">horizontal</item>
        <item name="android:padding">20dp</item>
        <item name="android:background">@xml/border2</item>
        <item name="android:layout_margin">2sp</item>
    </style>

</resources>